    <sql>BEGIN;
LOCK ${TABLE_NAME};

ALTER TABLE ${TABLE_NAME} RENAME TO ${TABLE_NAME}old;

CREATE TABLE ${TABLE_NAME} (LIKE ${TABLE_NAME}old INCLUDING DEFAULTS);
ALTER TABLE ${TABLE_NAME} OWNER TO dbadminlogs;
GRANT SELECT ON TABLE ${TABLE_NAME} TO ro;
GRANT SELECT, INSERT ON TABLE ${TABLE_NAME} TO rw;

ALTER SEQUENCE ${TABLE_NAME}_id_seq OWNED BY ${TABLE_NAME}.id;

ALTER TABLE ${TABLE_NAME}old ALTER COLUMN id DROP DEFAULT;
ALTER TABLE ${TABLE_NAME}old SET SCHEMA etl;
ALTER TABLE etl.${TABLE_NAME}old RENAME TO ${TABLE_NAME};

ALTER TABLE ${TABLE_NAME} ADD CONSTRAINT ${TABLE_NAME}_pkey PRIMARY KEY(id);
COMMIT;
</sql>
