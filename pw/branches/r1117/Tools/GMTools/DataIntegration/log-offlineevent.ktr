  id
, &apos;${LOCATION}&apos; as location 
, playerid
, persistentid
, code
, auxdata
, &quot;timestamp&quot;
, CASE WHEN timestamp &lt; &apos;2010-09-01&apos; OR timestamp &gt; &apos;2030-09-01&apos; THEN 0
       ELSE to_char(timestamp, &apos;YYYYMMDD&apos;)::integer END AS datehierarchy
FROM etl.offlineevent
where id &gt; ?
ORDER BY id</sql>
